<!DOCTYPE html>
<html>
<body>
<%- include("../common/header") %>

<section id="faq" class="faq section-bg">
    <div id="identified">
        <div class="container" data-aos="fade-up">

            <div class="section-title">
                <h2>공지사항</h2>
                <div id="marginup">
                    <form action="/notice/searchBoard" method="post" role="form" class="php-email-form" id="search">
                        <div id="buttongrid">
                            <input style="flex-shrink: 1.8; margin-right: 13px;" type="text" name="searchTitle"
                                   class="form-control" placeholder="제목을 검색하세요" required="">
                            <a class="mybtn" onclick="document.getElementById('search').submit();"
                               style="width:130px; flex-shrink:1.1;margin-right: 13px;" type="submit">검색</a>
                            <a class="mybtn" style="width:130px; flex-shrink:1.1;" href="/notice/writeBoard">작성</a>
                        </div>
                    </form>
                </div>
            </div>

            <div class="faq-list">
                <ul>

                    <%
                    for(var i = 0; i < results.length; i++)
                    {
                        var data = results[i];
                    %>

                    <li data-aos="fade-up" data-aos-delay="100">
                        <i class="bx bx-help-circle icon-help"></i> <a data-bs-toggle="collapse" class="collapse"
                                                                       data-bs-target="#faq-list-1"><a
                                    href="/notice/getBoard?id=<%= data.id %>"><b><%= data.title %></b></a><i
                                    class="bx bx-chevron-down icon-show"></i></a>
                        <div id="faq-list-1" class="collapse show" data-bs-parent=".faq-list">
                            <p><b>
                                    글번호:</b>
                                <%= data.id %>
                            </p>
                            <p><b>
                                    아이디:</b>
                                <%= data.userId %>
                            </p>
                            <p><b>
                                    최종수정시간:</b>
                                <%= data.updatedAt %>
                            </p>
                        </div>
                    </li>

                    <% } %>
                </ul>


            </div>

        </div>

</section>
</div>
</body>
<%- include("../common/footer") %>
</html>
